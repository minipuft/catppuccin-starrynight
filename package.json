{
  "name": "catppuccin-starrynight-spicetify-theme",
  "version": "1.0.0",
  "description": "Spicetify theme with Catppuccin StarryNight dynamic effects.",
  "main": "theme.js",
  "scripts": {
    "test": "jest",
    "build": "esbuild src-js/theme.entry.ts --bundle --outfile=theme.js --format=iife --target=es2020 --external:react --external:react-dom",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "list-selectors": "node scripts/listSelectors.js",
    "scan-css": "ts-node scripts/scanCssTokens.ts",
    "diff-css": "ts-node scripts/diffTokens.ts",
    "replace-css": "ts-node scripts/replaceCssTokens.ts --mapping build/css-audit/replace-map.json",
    "generate-map": "ts-node scripts/generateReplaceMap.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.21.4",
    "jest": "^29.7.0",
    "spcr-settings": "^1.3.1",
    "spicetify-creator": "^1.0.17",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "stylelint": "^15.10.3",
    "jsdom": "^22.1.0",
    "@types/jsdom": "^21.1.1",
    "ts-node": "^10.9.2",
    "postcss": "^8.4.38",
    "postcss-scss": "^4.0.6"
  },
  "keywords": [
    "spicetify",
    "theme",
    "catppuccin",
    "starrynight"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "idb": "^8.0.3"
  }
}
